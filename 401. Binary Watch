class Solution {
public:
int countones(int n){
    int cnt=0;
    while(n>0){
        cnt+=(n%2);
        n=n/2;
    }
    return cnt;
}
    vector<string> readBinaryWatch(int turnedOn) {
        vector<string>result;
        for(int h=0;h<12;h++){
            for(int m=0;m<60;m++){
                 if(countones(h)+countones(m)==turnedOn){
                    string time=to_string(h)+":";
                    if(m<10) time+="0";
                    time+=to_string(m);
                    result.push_back(time);
                 }
            }
        }
       return result;
    }
};
