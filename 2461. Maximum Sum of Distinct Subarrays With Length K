class Solution {
public:
    long long maximumSubarraySum(vector<int>& nums, int k) {
        long long sum=0;
        int left=0;
        long long maxsum=0;
        unordered_set<int>s;
        for(int right=0;right<nums.size();right++){
            while(s.count(nums[right])){
                s.erase(nums[left]);
                sum-=nums[left];
                left++;
            }
            s.insert(nums[right]);
            sum+=nums[right];
            if(right-left+1==k){
                maxsum=max(maxsum,sum);
                s.erase(nums[left]);
                sum-=nums[left];
                left++;
            }
        }
        return maxsum;
    }
};
