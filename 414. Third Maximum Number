class Solution {
public:
    int thirdMax(vector<int>& nums) {
        vector<int>unique;
        for(int i=0;i<nums.size();i++){
            int x=nums[i];
        
        if(find(unique.begin(),unique.end(),x)==unique.end()){
            unique.push_back(x);
        }}
        sort(unique.begin(),unique.end());
        if(unique.size()>=3){
            return unique[unique.size()-3];
        }
        return unique.back();
    }
};
