class Solution {
public:
    string reverseStr(string s, int k) {
        int n=s.size();
        for(int i=0;i<n;i+=2*k){
            int left=i;
            int r=min(i+k-1,n-1);
            while(left<r){
                swap(s[left],s[r]);
                left++;
                r--;

            }
        }
        return s;
    }
};
