class Solution {
public:
    void duplicateZeros(vector<int>& arr) {
        int n=arr.size ();
        int zeros=0;
        for(int i=0;i<n;i++){
            if(arr[i]==0){
                zeros++;
            }
        }int j=n-1+zeros;

        for(int i=n-1;i>=0;i--){
            if(j<n){
                arr[j]=arr[i];
            }
            if(arr[i]==0){
                j--;
                if(j<n){
                    arr[j]=0;
                }
            }
            j--;
        }
    }
};
