class Solution {
public:
    bool hasSameDigits(string s) {
        vector<int>dig;
        for(int i=0;i<s.size();i++){
            dig.push_back(s[i]-'0');
        }
        while(dig.size()>2){
            vector<int>r;
            for(int i=0;i<dig.size()-1;i++){
                int sum=(dig[i]+dig[i+1])%10;
                r.push_back(sum);
                            }
                            dig=r;
                                    }
                                    if(dig[0]==dig[1]){
                                        return true;
                                    }
                                    else{
                                    return false;}
    }
};
