class Solution {
public:
bool isnotzero(int num){
            while(num>0){
                if(num%10==0) return false;
                num=num/10;
            }
                return true;
            
        }
    vector<int> getNoZeroIntegers(int n) {
        for(int i=1;i<n;i++){
            int j=n-i;
            if(isnotzero(i) && isnotzero(j)){
                return {i,j};
            }
        }
        return{};
    }
};
